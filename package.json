{
  "name": "msa-api",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "scripts": {
    "build": "msa-api/node_modules/.bin/nest build",
    "build:all": "msa-api/node_modules/.bin/nest build --all",
    "format": "msa-api/node_modules/.bin/prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start": "NODE_ENV=local nest start msa-api --debug",
    "start:auth": "NODE_ENV=local nest start auth --debug --watch",
    "start:nexon": "NODE_ENV=local nest start nexon --watch",
    "start:user": "NODE_ENV=local nest start user --debug --watch",
    "start:scheduler": "NODE_ENV=local nest start scheduler --debug",
    "lint": "msa-api/node_modules/.bin/eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "lint:ci": "msa-api/node_modules/.bin/eslint -f ./node_modules/eslint-formatter-teamcity/index.js \"{src,apps,test}/**/*.ts\" --fix",
    "test": "msa-api/node_modules/.bin/jest --passWithNoTests --",
    "test:watch": "msa-api/node_modules/.bin/jest --watch",
    "test:cov": "msa-api/node_modules/.bin/jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register msa-api/node_modules/.bin/jest --runInBand",
    "test:e2e": "msa-api/node_modules/.bin/jest --config ./apps/msa-api/test/jest-e2e.json"
  },
  "lint-staged": {
    "{src,apps,libs,test}/**/*.ts": [
      "eslint --fix",
      "jest --passWithNoTests --bail --findRelatedTests --"
    ]
  },
