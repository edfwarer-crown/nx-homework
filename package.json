    "@types/jest": "^29.5.14",
    "@types/mime-types": "^2.1.4",
    "@types/node": "^22.13.14",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.29.1",
    "@typescript-eslint/parser": "^8.29.1",
    "eslint": "^9.24.0",
    "eslint-config-prettier": "^10.1.2",
    "eslint-formatter-teamcity": "^1.0.0",
    "eslint-import-resolver-typescript": "^3.10.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.6",
    "globals": "^15.15.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jiti": "^2.4.2",
    "lint-staged": "^15.5.0",
    "prettier": "^3.5.3",
    "prisma": "^5.22.0",
    "supertest": "^7.1.0",
    "ts-jest": "^29.3.1",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.29.1"
  },
  "jest": {
    "modulePathIgnorePatterns": [
      "<rootDir>/importer/"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/libs/",
      "<rootDir>/apps/"
    ],
    "moduleNameMapper": {
      "^@apps/auth(|/.*)$": "<rootDir>/apps/auth/src/$1",
      "^@apps/push(|/.*)$": "<rootDir>/apps/push/src/$1",
      "^@apps/scheduler(|/.*)$": "<rootDir>/apps/scheduler/src/$1",
      "^@apps/user(|/.*)$": "<rootDir>/apps/user/src/$1",
      "^@apps/nexon(|/.*)$": "<rootDir>/apps/nexon/src/$1",
      "^@libs/core(|/.*)$": "<rootDir>/libs/core/src/$1",
      "^@libs/fastify-multer(|/.*)$": "<rootDir>/libs/fastify-multer/src/$1",
      "^@libs/dto(|/.*)$": "<rootDir>/libs/dto/src/$1",
      "^@libs/i18n(|/.*)$": "<rootDir>/libs/i18n/src/$1",
      "^@libs/entity(|/.*)$": "<rootDir>/libs/entity/src/$1",
      "^@libs/utils(|/.*)$": "<rootDir>/libs/utils/src/$1",
      "^@libs/health(|/.*)$": "<rootDir>/libs/health/src/$1"
    }
  }
}
