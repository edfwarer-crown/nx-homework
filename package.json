    "@eslint/js": "^9.24.0",
    "@nestjs/cli": "^11.0.6",
    "@nestjs/schematics": "^10.2.3",
    "@nestjs/testing": "^10.4.7",
    "@onozaty/prisma-db-comments-generator": "^1.1.0",
